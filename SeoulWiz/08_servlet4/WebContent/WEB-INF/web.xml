<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>08_servlet4</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <context-param>
 	<param-name>id3</param-name>
 	<param-value>init3</param-value>
  </context-param>
  <context-param>
 	<param-name>pwd3</param-name>
 	<param-value>pass3</param-value>
  </context-param>
  
  <servlet>
    <servlet-name>InitParam</servlet-name>
    <servlet-class>kr.co.iamtek.InitParam</servlet-class>
    <init-param>
      <param-name>id1</param-name>
      <param-value>init1</param-value>
    </init-param>
    <init-param>
      <param-name>pwd1</param-name>
      <param-value>pass1</param-value>
    </init-param>
    <init-param>
      <param-name>path1</param-name>
      <param-value>\path1</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitParam</servlet-name>
    <url-pattern>/IParam</url-pattern>
  </servlet-mapping>
  
  <listener>
  	<listener-class>kr.co.iamtek.ContextListener</listener-class>
  </listener>
  
</web-app>